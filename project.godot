; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="yokai_watch_pixel"
run/main_scene="res://src/titlescreen/titlescreen.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://res/yokai/jibanyan/icon_export.png"

[autoload]

global_item="*res://src/global/global_item.gd"
yokai_stats="*res://res/yokai/stats/yokai_stats.gd"
global="*res://src/global/global.gd"
global_npc="*res://src/global/global_npc.gd"
global_yokai="*res://src/global/global_yokai.gd"
GlobalQuests="*res://src/global/global_quests.gd"
GlobalBattle="*res://src/global/global_battle.gd"

[display]

window/size/viewport_width=240
window/size/viewport_height=135
window/size/window_width_override=1200
window/size/window_height_override=675
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/bottom_panel_shortcuts/plugin.cfg", "res://addons/script-tabs/plugin.cfg")

[file_customization]

folder_colors={
"res://addons/": "gray",
"res://res/": "red",
"res://scn/": "teal",
"res://src/": "orange"
}

[filesystem]

import/blender/enabled=false

[importer_defaults]

csv_translation={
"compress": true,
"delimiter": 0
}

